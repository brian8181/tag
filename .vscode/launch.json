    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++ - MAKE DEBUGGDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": ["test/test1.mp3", "test/test2.mp3"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "MAKE DEBUGGDB",
            "miDebuggerPath": "/usr/bin/gdb"
        },
